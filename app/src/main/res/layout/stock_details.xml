<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/test"
        android:textStyle="bold"/>
    <com.db.chart.view.LineChartView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/linechart"
        android:paddingBottom="@dimen/medium_space"
        android:paddingEnd="@dimen/medium_space"
        android:paddingLeft="@dimen/medium_space"
        android:paddingRight="@dimen/medium_space"
        android:paddingStart="@dimen/medium_space"
        android:paddingTop="@dimen/medium_space"
        android:layout_width="match_parent"
        android:layout_height="@dimen/stock_height" />

</LinearLayout>